@model Experience
@{
    ViewData["Title"] = "CreateExperience";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Dashboard</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Dashboard Table</div>
					</div>
					<div class="card-body">

						<form method="post">
							<label>Job Experience</label>
							<input type="text" asp-for="Head" class="form-control" required />
							<br />
							<label>Position</label>
							<input type="text" asp-for="Title" class="form-control" required />
							<br />
							<label>Date Info</label>
							<input type="text" asp-for="Date" class="form-control" required />
							<br />
							<label>Description</label>
							<textarea rows="4" class="form-control" asp-for="Description" required></textarea>
							<br />
							<button class="btn btn-primary">Add Experience</button>
						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>


